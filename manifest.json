{
  "manifest_version": 2,

  "name": "LeetCode Add-ons",
  "description": "Small enhancements for LeetCode.",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },

  "content_scripts": [{
    "css": ["style.css"],
    "js": ["script.js"],
    "matches": ["https://leetcode.com/*"]
  }]
}
